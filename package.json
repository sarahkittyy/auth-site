  "scripts": {
    "build-client": "webpack --config webpack.client.config.js",
    "build-server": "webpack --config webpack.server.config.js",
    "build": "concurrently 'yarn run build-client' 'yarn run build-server'",
    "start": "yarn build && node build/server.js",
    "dev-client": "webpack-dev-server --config webpack.client.config.js",
    "dev": "concurrently 'yarn dev-server' 'yarn dev-client'",
    "dev-server": "webpack --watch --config webpack.server.dev.config.js",
    "run-server": "npm run build-server && nodemon build/server.js"
  }
